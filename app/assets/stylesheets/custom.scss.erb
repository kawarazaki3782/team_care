@import "bootstrap-sprockets";
@import "bootstrap";

// 色
$color-main: #8eb8af; // ベース色
$color-main2: #eebeb4; // ベース色2
$color-main3: #f3c39f; //ベース色3
$color-active: #e81919; // アクティブ色
$color-text: #333; // テキスト色
$gray-medium-light: #eaeaea;

$layout-width-inner: 1200px;
/* mixins, variables, etc. */

// ブレークポイント
$breakpoints: (
  "sm": "(max-width: 375px)",
  "sp": "(min-width: 376px) and (max-width: 767px)",
  "tab": "(min-width: 768px) and (max-width: #{$layout-width-inner})",
  "pc": "(min-width: #{$layout-width-inner})"
);

// メディアクエリー
@mixin mq($breakpoint: sp) {
	@media #{map-get($breakpoints, $breakpoint)}{
		@content;
	}
}

*,
*::before,
*::after {
	box-sizing: border-box;
}

html,
body {
  	margin: 0;
  	padding: 0;
  	overflow-x: hidden!important;
}

body{
	font-family: "游ゴシック体", YuGothic, "游ゴシック Medium", "Yu Gothic Medium", "游ゴシック", "Yu Gothic", sans-serif;
	font-size: 16px;
	line-height: 1.6;
	background: #fff;
	color: $color-text;
    padding-top: 150px;
  }

img {
	max-width: 100%;
	height: auto;
	border: none;
}

ul,
ol {
	list-style: none;
	padding: 0;
	margin: 0;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	padding: 0;
	margin: 0;
 }

 a {
	color: inherit;
 }

//////////////////////header/////////////////////////// 

.inner {
	width: $layout-width-inner;
	max-width: 100%;
	margin: 0 auto;

}

.header {
	background: $color-main;
	height: 100px;
	position: fixed;
	width: 100%;
	box-shadow: 0 3px 3px (rgba#000 0.16);
	z-index: 100;
	top: 0;
	left: 0;

  @include mq(sp) {
	height: 100px;
}

  @include mq(sm) {
	height: 120px;
 }
}

.header＿inner {
	display: flex;
	align-items: center;
	height: inherit;
}

#hamburger .btn-gNav span {
  @include mq(sm) {
	position: absolute;
	width: 100%;
	height: 4px;
	background: #666;
	border-radius: 10px;
	-webkit-transition: all 400ms;
	transition: all 400ms;
}

  @include mq(sp) {
	position: absolute;
	width: 100%;
	height: 4px;
	background: #666;
	border-radius: 10px;
	-webkit-transition: all 400ms;
	transition: all 400ms;
  }
}

#hamburger .btn-gNav span:nth-child(1) {
  @include mq(sm) {
	top:0;
}

  @include mq(sp) {
	top:0;
 }
}

