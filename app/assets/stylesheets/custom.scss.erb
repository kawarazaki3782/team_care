////////////////////ホーム ログインなし///////////////////////////

.main {
    background: image-url('main.jpeg') no-repeat center center / cover;
	width: 100%;
	height: 600px;
	position: relative;
 	margin-bottom: 50px;
 	border-radius: 20px;
}

.main::before {
	content: '';
    background-color: rgba(0, 0, 0, 0.4);
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: block;
    border-radius: 20px;
}


.main_visual {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	text-align: center;
  	color: #fff;
}

.main_visual_title {
	font-size: 60px;
	font-weight: 600;
	line-height: 1.3;

  @include mq('sm') {
	font-size: 30px;
}	

  @include mq('sp') {
	font-size: 38px;
 }		
} 


.main_visual_lead {
	font-size: 40px;
	font-weight: 600;
	margin-top: 24px;

  @include mq('sm') {
	font-size: 20px;
}		

  @include mq('sp') {
	font-size: 20px;
 }		
}

.main_visual_btn {
	margin-top: 40px;

a {
	display: inline-block;
	width: 384px;
	max-width: 100%;
	line-height: 72px;
	height: 72px;
	text-align: center;
	text-decoration: none;
	background: $color-main2;
	color: #fff;
	font-size: 24px;
	font-weight: 700;
	border-radius: 8px;
	cursor: pointer;
	outline: none;
	padding: 0rem 2rem;
	box-shadow: 2px 2px 4px gray;
	border: 1px solid #ccc;

  @include mq('sm') {
	width: 300px;
}		
  @include mq('sp') {
	width: 230px;
  }		
 }
}

.main_visual_btn a:active {
	transition: 0.1s;
	transform: translate3d(0,6px,0);
	box-shadow: none;
}

//////////////////////about///////////////////////////

.about_main {
	background: image-url('hands.jpeg') no-repeat center center / cover;
	width: 100%;
	height: 600px;
	position: relative;
    margin-bottom: 50px;
    border-radius: 20px;

  @include mq('sp') {
	height: 300px;
  }		
  @include mq('sm') {
	height: 300px;
  }		
}

.breadcrumb {
	padding: 0 10px 10px 30px;
    margin-bottom: 20px;
    list-style: none;
    background-color: #f5f5f5;
    border-radius: 4px;
}

ul.breadcrumb__list {
    display: flex;
	padding-top: 15px;
}

li.features-item {
    display: flex;
}

.features {
    position: relative;
    padding: 90px 20px;
}

.util-ttl {
    font-size: 32px;
    line-height: 1.2;
    font-weight: bold;
    text-align: center;

  @include mq('sp') {
