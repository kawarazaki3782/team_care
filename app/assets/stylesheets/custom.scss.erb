    text-justify: inter-ideograph;

  @include mq('sp') {
	font-size: 10px;
 }
  @include mq('sm') {
	font-size: 10px;
 }		
}

.features-item:nth-child(2):before {
    content: "";
    display: block;
    width: 120px;
    height: 120px;
