 }	
  @include mq('sm') {
	font-size: 18px;
 }	
}

.util-ttl-inner:after {
    content: '';
    display: block;
    height: 12px;
    width: 70px;
    background: linear-gradient(to right, #eebe64, #f3c39f);
    border-radius: 20px;
    margin: 20px auto 0;
}

.util-ttl:after {
    display: block;
    content: attr(data-title);
    font-family: "Work Sans", sans-serif;
    font-size: 18px;
    letter-spacing: 0;
    margin-top: 24px;
    text-transform: uppercase;
}

.features-list {
    max-width: 870px;
    margin: 56px auto 0;
}

.features-item:nth-child(1):before {
    content: "";
    display: block;
    width: 120px;
    height: 120px;
    flex: 0 0 120px;
    background: url('noun_Use.png') center center/contain no-repeat;
}

.features-item-inner1{
    flex: 0 0 calc(100% - 170px);
    padding-left: 50px;
    padding-top: 18px;
}

.features-item-inner2{
    flex: 0 0 calc(100% - 170px);
    padding-left: 50px;
    padding-top: 11px;
}

.features-item-inner3{
    flex: 0 0 calc(100% - 170px);
    padding-left: 50px;
    padding-top: 7px;
}

.features-item-ttl {
    font-size: 18px;
    font-weight: bold;
    line-height: 1.5;
	
  @include mq('sp') {
	font-size: 14px;
 }
  @include mq('sm') {
	font-size: 14px;
 }
}

.features-item-text {
    line-height: 1.8;
    margin-top: 20px;
